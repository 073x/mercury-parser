{
  "presets": ["es2015"],
  "plugins": [
    "transform-runtime",
    "transform-es2015-destructuring",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "./utils": "utils",
        "./cleaners": "cleaners",
        "./resource": "resource",
        "./extractors": "extractors",
        "./test-helpers.js": "test-helpers",
        "./iris.js": "iris"
      }
    }]
  ]
}
